                            {"value": "9", "displayName": "9"}
                        ]
                    }
                },
                "connectorColor": {
                    "displayName": "Connector Color",
                    "type": { "fill": { "solid": { "color": true }} }
                },
                "connectorLength": {
                    "displayName": "Connector Length",
                    "description": "Length of connector",
                    "type": { 
                        "integer": true 
                    }
